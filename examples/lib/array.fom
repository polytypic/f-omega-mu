include "array" in

let type array = λα.∃array.array α in

let tabulate = target[∀α.(int → α) → int → array α]
  "fn => n => {const a = new Array(n); for (let i=0; i<n; ++i) a[i] = fn(i); return a}" in

let length = target[∀α.array α → int] "a => a.length" in

let sub = target[∀α.array α → int → α]
  "a => i => { if (i < 0 || a.length <= i) throw `Array.sub: Index out of bounds: 0 <= ${i} < ${a.length}`; return a[i] }" in

let update = target[∀α.array α → int → α → ()]
  "a => i => v => { if (i < 0 || a.length <= i) throw `Array.update: Index out of bounds: 0 <= ${i} < ${a.length}`; a[i] = v }" in

《array\{tabulate, length, sub, update}》: ∃(Array)
