# First-class type equality witnesses

type Leibniz = λα.λβ.∀φ.φ α → φ β
type Fold = λα.λβ.∀φ.(∀γ.φ γ γ) → φ α β

type Eq = λeq.{
  ofFold: ∀α.∀β.Fold α β → eq α β
  ofLeibniz: ∀α.∀β.Leibniz α β → eq α β
  refl: ∀α.eq α α
  symm: ∀α.∀β.eq α β → eq β α
  trans: ∀α.∀β.∀γ.eq β γ → eq α β → eq α γ
  to: ∀α.∀β.eq α β → α → β
  from: ∀α.∀β.eq α β → β → α
}

let Leibniz: Eq Leibniz = {
  ofFold = Λ_.Λ_.λab.Λφ.ab«λα.λβ.φ α → φ β» Λ_.λx.x
  ofLeibniz = Λ_.Λ_.λab.ab
  refl = Λ_.Λ_.λx.x
  symm = Λα.Λ_.λab.Λφ.ab«λβ.φ β → φ α» λx.x
  trans = Λ_.Λ_.Λ_.λbc.λab.Λφ.λx.bc«φ» ab«φ»(x)
  to = Λ_.Λ_.λab.ab«λx.x»
  from = Λα.Λ_.λab.ab«λβ.β → α» λx.x
}

let Fold: Eq Fold = {
  ofFold = Λ_.Λ_.λab.ab
  ofLeibniz = Λα.Λ_.λab.Λφ.λh.ab«φ α» h«α»
  refl = Λα.Λ_.λf.f«α»
  symm = Λ_.Λ_.λab.Λφ.λf.ab«λα.λβ.φ β α» f
  trans = Λα.Λ_.Λ_.λbc.λab.Λϕ.λf.bc«λγ.λδ.ϕ α γ → ϕ α δ» (Λ_.λx.x) ab«ϕ»(f)
  to = Λ_.Λ_.λab.ab«λα.λβ.α → β» Λ_.λx.x
  from = Λ_.Λ_.λab.ab«λα.λβ.β → α» Λ_.λx.x
}

{Leibniz, Fold}
